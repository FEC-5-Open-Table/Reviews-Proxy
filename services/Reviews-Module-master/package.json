    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "chart.js": "^2.7.2",
    "chartjs": "^0.3.24",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
